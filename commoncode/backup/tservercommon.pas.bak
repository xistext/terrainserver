unit TServerCommon;

interface

type tmsgtype = byte;

const msg_undefined = 0;
      msg_ping      = 1;
      msg_string    = 2;
      msg_binary    = 3;
      msg_tile      = 4;

type TMsgHeader = record
        msgtype : tmsgtype;
        msglen  : dword;
      end;

     TTileHeader = packed record
        TileX, TileY : integer; { position of tile within world }
        TileW, TileH : integer; { dimensions of tile }
      end;

implementation

end.
